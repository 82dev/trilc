program ]> stmt* EOF

stmt ]> block
     |  dec
     |  newAss
     |  ';'

block ]> '{'
            stmt*
         '}'

expr ]> term

term ]> factor ('+'|'-' factor)*
     |  factor

factor ]> unary ('*'|'/' unary)*
       |  primary

unary ]> ('!'|'-') unary
      |  primary

primary ]> NUM
        |  TRUE
        |  FALSE
        |  STRING