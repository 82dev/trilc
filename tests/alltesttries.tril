..every Tril file starts with '{' and ends with '}'
{
    ..import lib
    use std;
    ..aliasing
    alias std.Output.Console as console;
    ..variables
    x:int = 4;
    ..methods and arrays
    main:null = pb([string] cmdargs){
        console.WriteLine(x.string());
        x = 4;
        console.WriteLine(x);
    }
    ..enums
    enum enumEx{
        a,
        b,
        c
    }
    ..structs
    struct coords{
        x:int;
        y:int;
        ctor(b:int,c:int){
            this.x = b;
            this.y = c;
        }
    }
    ..classes
    class classEx{
        printHi:null = pb(){
            console.WriteLine("Hey!");
        }
    }
    
    ..only one inheritation as multiple parents can mess up type trees
    class inheritEx <| classEx
    {
        printHi:null = pb(){
            base.printHi();
        }
    }
}